//preorder: root->left->right
class Solution {
public:
    vector<int> ans;
    void preorder(TreeNode* root) 
    {
         
        if(root == NULL) return;

        ans.push_back(root->val);
        if( root->left != NULL) preorder(root->left);
        if( root->right != NULL) preorder(root->right);
    }

    vector<int> preorderTraversal(TreeNode* root)
    {
        preorder(root);
        return ans;
    }
};
