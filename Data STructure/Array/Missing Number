// You are given 
class Solution {
public:
    int missingNumber(vector<int>& nums) 
    {
       int sum = 0, total_sum = 0, missing;
       int n = nums.size();
       for(int i = 0; i < n; i++)
       {
           sum1 = sum1 + nums[i]; 
       }  

       total_sum = n*(n+1) / 2;
       missing = total_sum - sum; 
       return missing;

    }
};
